<%- include("partials/header"); %>
<div class="row mb-4">
    <h2>Add New Device</h2>
    
</div>






   
<div class="container-fluid">
    <div class="row">
     
      <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
      <form action="/createStream/<%=StreamID%>" method="POST">

        <div class="mb-3">
          <label class="form-label">Stream</label>
          <input type="text" class="form-control" name="directory_id" value="<%=StreamID%>" readonly="readonly">
        </div>


        <div class="mb-3">
            <label class="form-label">Name</label>
            <input type="text" class="form-control" name="name">
            
        </div>
        <div class="mb-3">
            <label class="form-label">Var Name</label>
            <input type="text" class="form-control" name="var_name">
            
        </div>
        <div class="mb-3">
            <label class="form-label">Display Name</label>
            <input type="text" class="form-control" name="display_name">
            
        </div>
        <div class="mb-3">
            <label class="form-label">Description</label>
            <input type="text" class="form-control" name="description">
            
        </div>
        <div class="mb-3">
            <label class="form-label">Units</label>
            <input type="text" class="form-control" name="units">
            
        </div>
        <div class="mb-3">
            <label class="form-label">Type</label>
            <input type="text" class="form-control" name="type">
            
        </div>
    
        <!-- <div class="mb-3">
          <label for="exampleInputPassword1" class="form-label">Top Directory</label>
          <input type="text" class="form-control" name="TopDir">
        </div> -->
        
        <button type="submit" class="btn">Submit</button>
      </form>
    </main>
    
    </div>
    
</div>
      




<%- include("partials/footer"); %>