<%- include("partials/header"); %>
<div class="row mb-4">
  <h2>Directories</h2>
</div>

<div class="container-fluid">
  <div class="row">
    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
      
<div class="=row">
    <% for(i in homeDirectory){ %>
       <% var temp = homeDirectory.directory %>
        <% for( var j = 0; j < temp.length; j++) { %>
            
    <div class="col mb-4 ">    
        
        <div class="card">
            
          <div class="card-body">
            <p class="card-text"> Name:  <%= temp[j].name %> </p>
            <p class="card-text"> Unique ID: <%= temp[j].id %> </p>
            
          </div>
          
        
           
        </div>
    </div>  
        <% } %>  
        <% break %>
    <% } %>
</div>
</main>
</div>
</div>




<%- include("partials/footer"); %>
