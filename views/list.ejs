<%- include("partials/header"); %>
      <h1>This is our list</h1>   

      
  
<div class = "row">
  <% for(var i = 0; i < currentList.length; i++){ %>
  <div class="col mb-4 ">
  <div class="card mb-4 shadow-sm" style="width: 18rem;">
    <!--- <img src="..." class="card-img-top" alt="..."> -->
    <div class="card-body">
      <h5 class="card-title">Name: <%= currentList[i].var_name %></h5>
      
      <p class="card-text">Device: <%= currentList[i].display_name %></p>
      <p class="card-text">Device: <%= currentList[i].description %></p>
      <p class="card-text">Device: <%= currentList[i].units %></p>
      <p class="card-text">Device: <%= currentList[i].type %></p>
     
      <a href="/" class="btn btn-primary">Go somewhere</a>
    </div>
  </div>
  </div>
  <% } %>


</div>





<%- include("partials/footer"); %>
